{
  "name": "e-surveyor",
  "version": "1.0.0",
  "build": "1",
  "scripts": {
    "start": "node server.js",
    "start:dev": "webpack-dev-server --config webpack.config.js --content-base build --port 8000",
    "build": "webpack --display minimal",
    "build:production": "grunt",
    "deploy": "vercel --prod -A other/now.json",
    "clean": "rm -rf build resources",
    "test": "jest --silent",
    "test:watch": "jest --watchAll",
    "test:eslint": "eslint src --quiet && echo '\\033[37;42;1m PASS \\033[00m'",
    "test:acceptance": "cd other && newman run postman_collection.json --bail --env-var plantnet_api_key=${PLANT_NET_API_KEY}",
    "fetch:data": "cd src/common/data && mkdir -p remote && node make.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run test:eslint"
    }
  },
  "dependencies": {
    "@bit/flumens.apps.attr-page": "0.6.5",
    "@bit/flumens.apps.date": "^0.1.0",
    "@bit/flumens.apps.device": "0.2.1",
    "@bit/flumens.apps.header": "0.6.2",
    "@bit/flumens.apps.helpers.alert": "0.4.1",
    "@bit/flumens.apps.helpers.loader": "0.1.1",
    "@bit/flumens.apps.helpers.toast": "0.2.0",
    "@bit/flumens.apps.info-background-message": "0.1.2",
    "@bit/flumens.apps.main": "0.2.0",
    "@bit/flumens.apps.menu-attr-item": "^0.5.4",
    "@bit/flumens.apps.modal-header": "^0.1.1",
    "@bit/flumens.apps.model-location-map": "1.0.1",
    "@bit/flumens.apps.models.drupal-user-model": "^0.5.6",
    "@bit/flumens.apps.models.init-stored-samples": "^0.1.0",
    "@bit/flumens.apps.models.media": "^0.2.4",
    "@bit/flumens.apps.models.model": "^0.1.0",
    "@bit/flumens.apps.models.occurrence": "^0.2.5",
    "@bit/flumens.apps.models.sample": "^0.2.9",
    "@bit/flumens.apps.models.store": "^0.3.1",
    "@bit/flumens.apps.page": "0.2.0",
    "@bit/flumens.apps.route-with-models": "^0.2.0",
    "@bit/flumens.apps.section": "^0.4.0",
    "@bit/flumens.apps.utils.location": "^0.1.0",
    "@capacitor/android": "^2.4.2",
    "@capacitor/core": "^2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@flumens/fetch-onedrive-excel": "^0.1.0",
    "@indicia-js/core": "5.4.0",
    "@indicia-js/remote": "^5.6.4",
    "@ionic-native/core": "5.29.0",
    "@ionic/core": "5.4.1",
    "@ionic/react": "5.4.1",
    "@ionic/react-router": "5.4.1",
    "@sentry/browser": "5.27.2",
    "@sentry/node": "^5.15.5",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-sqlite-storage": "^5.1.0",
    "express": "4.17.1",
    "express-history-api-fallback": "2.2.1",
    "html-webpack-plugin": "4.5.0",
    "http-proxy-middleware": "^1.0.3",
    "i18next": "^19.8.4",
    "ionicons": "5.2.3",
    "json-loader": "^0.5.7",
    "leaflet": "^1.6.0",
    "localforage": "1.9.0",
    "localforage-cordovasqlitedriver": "1.8.0",
    "lodash": "^4.17.15",
    "log4js": "^6.3.0",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "mobx-react-devtools": "6.1.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-countup": "^4.3.3",
    "react-dom": "17.0.1",
    "react-i18next": "^11.7.3",
    "react-router": "^5.1.0",
    "react-router-dom": "5.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@capacitor/cli": "^2.4.2",
    "@flumens/eslint-config": "^0.1.1",
    "@flumens/has-env": "1.0.0",
    "autoprefixer": "^9.7.3",
    "axios": "0.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "cordova-res": "^0.15.2",
    "css-loader": "4.3.0",
    "csv": "5.3.2",
    "dotenv": "8.2.0",
    "eslint": "7.10.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.1.0",
    "grunt": "1.3.0",
    "grunt-cli": "1.3.2",
    "grunt-exec": "3.0.0",
    "grunt-prompt": "1.3.3",
    "husky": "4.3.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "load-grunt-config": "3.0.1",
    "load-grunt-tasks": "5.1.0",
    "mini-css-extract-plugin": "0.11.3",
    "mock-geolocation": "1.0.11",
    "newman": "^5.0.1",
    "node-sass": "4.14.1",
    "p-queue": "6.3.0",
    "postcss-loader": "3.0.0",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "10.0.2",
    "sharp-cli": "1.14.1",
    "sinon": "9.1.0",
    "string-replace-loader": "2.3.0",
    "style-loader": "1.3.0",
    "url-loader": "4.1.0",
    "vercel": "20.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-shell-plugin-next": "1.2.0"
  },
  "engines": {
    "node": "14",
    "npm": "6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlumensIO/e-surveyor-app"
  },
  "bugs": {
    "url": "https://github.com/FlumensIO/e-surveyor-app/issues"
  },
  "licenses": [
    {
      "type": "GNU GPL v3",
      "url": "http://www.gnu.org/licenses/gpl.html"
    }
  ],
  "keywords": [
    "biological",
    "recording",
    "mobile",
    "app",
    "offline",
    "ceh"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-exponentiation-operator",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "extends": "@flumens",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./webpack.config.js"
        }
      }
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.14"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "jest": {
    "globals": {
      "__ENV__": "test",
      "__DEV__": false,
      "__TEST__": true
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "((\\.|/*.)(spec))\\.js?$",
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "^@apps$": "<rootDir>/src/common/appsBitCollection.js",
      "^config$": "<rootDir>/src/common/config/config.js",
      "^helpers/(.*)$": "<rootDir>/src/common/helpers/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic-native|@ionic|@bit)"
    ]
  }
}
