{
  "version": 2,
  "builds": [
    { "src": "./build/**", "use": "@now/static" },
    { "src": "./other/demo.html", "use": "@now/static" }
  ],
  "routes": [
    { "src": "^/demo.html", "dest": "other/demo.html" },
    { "src": "^/demo_(.*)", "dest": "other/demo_$1" },
    { "src": "^/(.*)", "dest": "build/$1" }
  ]
}
